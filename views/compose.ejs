    <form class="" action="/compose" method="post">
        <div class="form-group">
            <h1>Compose</h1>
            <h4>Title</h4>
            <input type="text" class="form-control" id="exampleFormControlInput1" name="postTitle" placeholder="Enter Title...">
        </div>
        <div class="form-group">
          <h4>Post</h4>
          <textarea class="form-control" id="exampleFormControlTextarea" col="30" rows="5" name="postBody" placeholder="Compose your comment..."></textarea>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
    
